<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="rwcjom.awit.com.rwcjo_m.fragments.DataSyncFragment">

    <!-- TODO: Update blank fragment layout -->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:padding="10dp"
        android:gravity="center">
    <TextView android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/logineduser_label"
        android:textSize="16sp"
        android:textColor="@color/listTextColor"/>
    <TextView android:id="@+id/logined_username"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="16sp"
        android:textColor="@color/listTextColor"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <com.dd.CircularProgressButton
            android:id="@+id/data_sync_dl_all"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:textColor="@color/cpb_white"
            android:textSize="18sp"
            app:cpb_cornerRadius="36dp"
            app:cpb_textComplete="@string/data_sync_dl_complete_btn_text"
            app:cpb_textError="@string/data_sync_dl_error_btn_text"
            app:cpb_textIdle="@string/data_sync_dl_all_btn_text"
            app:cpb_selectorIdle="@drawable/idle_state_selector"
            app:cpb_selectorComplete="@drawable/complete_state_selector"
            app:cpb_selectorError="@drawable/error_state_selector"/>

        <com.dd.CircularProgressButton
            android:id="@+id/data_sync_delete_all"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:textColor="@color/cpb_white"
            android:textSize="18sp"
            app:cpb_cornerRadius="36dp"
            app:cpb_textComplete="@string/data_sync_delete_complete_btn_text"
            app:cpb_textError="@string/data_sync_delete_error_btn_text"
            app:cpb_textIdle="@string/data_sync_delete_all_btn_text"
            app:cpb_selectorIdle="@drawable/idle_state_selector"
            app:cpb_selectorComplete="@drawable/complete_state_selector"
            app:cpb_selectorError="@drawable/error_state_selector"/>

    <com.dd.CircularProgressButton
        android:id="@+id/data_sync_dl_section"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="32dp"
        android:layout_marginRight="32dp"
        android:textColor="@color/cpb_white"
        android:textSize="18sp"
        app:cpb_cornerRadius="36dp"
        app:cpb_textComplete="@string/data_sync_dl_complete_btn_text"
        app:cpb_textError="@string/data_sync_dl_error_btn_text"
        app:cpb_textIdle="@string/data_sync_dl_section_btn_text"
        app:cpb_selectorIdle="@drawable/idle_state_selector"
        app:cpb_selectorComplete="@drawable/complete_state_selector"
        app:cpb_selectorError="@drawable/error_state_selector"/>
    <com.dd.CircularProgressButton
        android:id="@+id/data_sync_dl_face"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="32dp"
        android:layout_marginRight="32dp"
        android:textColor="@color/cpb_white"
        android:textSize="18sp"
        app:cpb_cornerRadius="36dp"
        app:cpb_textComplete="@string/data_sync_dl_complete_btn_text"
        app:cpb_textError="@string/data_sync_dl_error_btn_text"
        app:cpb_textIdle="@string/data_sync_dl_face_btn_text"
        app:cpb_selectorIdle="@drawable/idle_state_selector"
        app:cpb_selectorComplete="@drawable/complete_state_selector"
        app:cpb_selectorError="@drawable/error_state_selector"/>
    <com.dd.CircularProgressButton
        android:id="@+id/data_sync_dl_testPoint"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="32dp"
        android:layout_marginRight="32dp"
        android:textColor="@color/cpb_white"
        android:textSize="18sp"
        app:cpb_cornerRadius="36dp"
        app:cpb_textComplete="@string/data_sync_dl_complete_btn_text"
        app:cpb_textError="@string/data_sync_dl_error_btn_text"
        app:cpb_textIdle="@string/data_sync_dl_testPoint_btn_text"
        app:cpb_selectorIdle="@drawable/idle_state_selector"
        app:cpb_selectorComplete="@drawable/complete_state_selector"
        app:cpb_selectorError="@drawable/error_state_selector"/>



    <com.dd.CircularProgressButton
        android:id="@+id/data_sync_dl_person"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="32dp"
        android:layout_marginRight="32dp"
        android:textColor="@color/cpb_white"
        android:textSize="18sp"
        app:cpb_cornerRadius="36dp"
        app:cpb_textComplete="@string/data_sync_dl_complete_btn_text"
        app:cpb_textError="@string/data_sync_dl_error_btn_text"
        app:cpb_textIdle="@string/data_sync_dl_person_btn_text"
        app:cpb_selectorIdle="@drawable/idle_state_selector"
        app:cpb_selectorComplete="@drawable/complete_state_selector"
        app:cpb_selectorError="@drawable/error_state_selector"/>


    <com.dd.CircularProgressButton
        android:id="@+id/data_sync_dl_basePoint"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="32dp"
        android:layout_marginRight="32dp"
        android:textColor="@color/cpb_white"
        android:textSize="18sp"
        app:cpb_cornerRadius="36dp"
        app:cpb_textComplete="@string/data_sync_dl_complete_btn_text"
        app:cpb_textError="@string/data_sync_dl_error_btn_text"
        app:cpb_textIdle="@string/data_sync_dl_basePoint_btn_text"
        app:cpb_selectorIdle="@drawable/idle_state_selector"
        app:cpb_selectorComplete="@drawable/complete_state_selector"
        app:cpb_selectorError="@drawable/error_state_selector"/>


    <com.dd.CircularProgressButton
        android:id="@+id/data_sync_dl_line"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="32dp"
        android:layout_marginRight="32dp"
        android:textColor="@color/cpb_white"
        android:textSize="18sp"
        app:cpb_cornerRadius="36dp"
        app:cpb_textComplete="@string/data_sync_dl_complete_btn_text"
        app:cpb_textError="@string/data_sync_dl_error_btn_text"
        app:cpb_textIdle="@string/data_sync_dl_line_btn_text"
        app:cpb_selectorIdle="@drawable/idle_state_selector"
        app:cpb_selectorComplete="@drawable/complete_state_selector"
        app:cpb_selectorError="@drawable/error_state_selector" />
        </LinearLayout>
</LinearLayout>
